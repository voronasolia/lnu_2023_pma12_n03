#include <iostream> 
#include <math.h> 
using namespace std; 
 
int main() { 
    float x, h = 0.1, y; 
    float a = 1, b = 2; 
    float min = 0, max = 0; 
 
    cout << "X \t\t\t\t Y\n"; 
 
    for (x = a; x <= b; x += h) { 
        y = cos(x) / sin(x) + 1; 
        cout << x << "\t\t\t\t" << y << "\n"; 
     
   //min y and max y 
 
    if (y < min) 
    { 
        min = y; 
    } 
    if (max < y) 
    { 
        max = y; 
    } 
} 
cout << "Min and max: " << min * max << endl; 
}
