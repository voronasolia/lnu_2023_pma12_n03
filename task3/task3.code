#include <iostream> 

using namespace std;

int main()
{
    int n;
    cout << "Enter n:";
    cin >> n;
    int* pArray;
    pArray = new int[n];
    for (int i = 0; i < n; i++)
    {
        cin >> pArray[i];
    }

    cout << "pArray:\n";
    for (int i = 0; i < n; i++)
    {
        if (pArray[i] <= i)
        {
            cout << pArray[i]<< "\t\t\t\t"<<"\n";
        }
        
    }
}
